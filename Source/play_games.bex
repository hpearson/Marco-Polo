StartGame:
 sleep 30
 CDPAUSE

 who = 0 						   'Start listning their frequency
 YourSound = 500				   'What you start on
 TheirSound = 5
 TheirSound = TheirSound * rnd(200) 'generate their number
 if TheirSound = 0 then
 	TheirSound = 5
 endif
 
 
 
 
GameLoop:
 if who = 1 then
 	psgvol 0,0
 	psgvol 1,10
 endif
 
 if who = 0 then
 	psgvol 0,10
 	psgvol 1,0
 endif

 psg 0,TheirSound
 psg 1,YourSound
 
 while 1
	 JoyStickInput=joypad(0)
	 
	 'StartButton
	 if JoyStickInput = 128 then
	 	goto EndGame
	 endif
	 
	 'UpButton
	 if JoyStickInput = 257 then
	 	goto UpButton
	 endif
	 
	 'DownButton
	 if JoyStickInput = 514 then
	 	goto DownButton
	 endif
	 
	 'AButton
	 if JoyStickInput = 64 then
	 	goto AButton
	 endif	 
	 
	 'CButton
	 if JoyStickInput = 32 then
	 	goto CButton
	 endif	 	
 wend
 
UpButton:
 YourSound = YourSound + 5;
 'Set input limiters
 if YourSound > 1000 then
 	YourSound = 1000
 endif	
 sleep 10
 goto GameLoop 

DownButton:
 YourSound = YourSound - 5;
 if YourSound < 5 then
	YourSound = 5
 endif	
 sleep 10
 goto GameLoop 
 
AButton:
 Who = 0
 goto GameLoop 

CButton:
 Who = 1
 goto GameLoop 
 
 
 
 
EndGame: 
 sleep 30
 psgvol 0,0
 psgvol 1,0
 if YourSound = TheirSound then
	'win
	WinCounter = WinCounter + 1
	goto WinCount
 else
 	'lose
	LostTheGame = 1
 	goto WinCount
 endif
 
WinCount:
 'Will beep out wins
 WinCounter2 = WinCounter
 while WinCounter > 0
 	psgvol 0,10
 	psg 0,2000
 	sleep 10
 	psgvol 0,0
 	sleep 10
 	WinCounter = WinCounter - 1
 wend
 'will long beep failure and reset wins
 WinCounter = WinCounter2
 sleep 60
 if LostTheGame = 1 then
 	LostTheGame = 0
 	WinCounter = 0
 	psgvol 0,10
 	psg 0,100
 	sleep 50
 	psgvol 0,0
 	sleep 10
 endif
 goto StartGame